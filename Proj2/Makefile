# ESTA MAKEFILE REMOVE OS FICHEIROS *.O E OS *.TXT POR QUESTOES DE ORGANIZAÇÃO
# FAVOR RETIRAR CASO SEJA INDESEJAVEL
all: ESCRITOR ESCRITOR-PAI Remove

ESCRITOR: ESCRITOR.o 
	gcc -o ESCRITOR ESCRITOR.o

ESCRITOR.o: ESCRITOR.c
	gcc -g -c ESCRITOR.c

ESCRITOR-PAI: ESCRITOR-PAI.o
	gcc -o ESCRITOR-PAI ESCRITOR-PAI.o

ESCRITOR-PAI.o: ESCRITOR-PAI.c
	gcc -g -c ESCRITOR-PAI.c

Remove:
	rm *.o *.txt
