all: LEITOR1-PAI LEITOR1 LEITOR2 LEITOR3
	
LEITOR1: LEITOR1.o 
	gcc -o LEITOR1 LEITOR1.o

LEITOR1.o: LEITOR1.c
	gcc -g -c LEITOR1.c

LEITOR1-PAI: LEITOR1-PAI.o
	gcc -o LEITOR1-PAI LEITOR1-PAI.o

LEITOR1-PAI.o: LEITOR1-PAI.c
	gcc -g -c LEITOR1-PAI.c
	
LEITOR2: LEITOR2.o 
	gcc -pthread -o LEITOR2 LEITOR2.o

LEITOR2.o: LEITOR2.c
	gcc -g -c LEITOR2.c
	
LEITOR3: LEITOR3.o 
	gcc -pthread -o LEITOR3 LEITOR3.o

LEITOR3.o: LEITOR3.c
	gcc -g -c LEITOR3.c




