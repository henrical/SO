all: WRITER WRITER-PARENT READER

WRITER: WRITER.o 
	gcc -o WRITER WRITER.o

WRITER.o: WRITER.c
	gcc -g -c WRITER.c

WRITER-PARENT: WRITER-PARENT.o
	gcc -o WRITER-PARENT WRITER-PARENT.o

WRITER-PARENT.o: WRITER-PARENT.c
	gcc -g -c WRITER-PARENT.c

READER: READER.o
	gcc -o READER READER.o

READER.o: READER.c
	gcc -g -c READER.c
